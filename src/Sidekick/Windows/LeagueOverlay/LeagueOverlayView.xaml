<Window
    x:Class="Sidekick.Windows.LeagueOverlay.LeagueOverlayView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Sidekick.Windows.LeagueOverlay"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:r="clr-namespace:Sidekick.Localization.Leagues;assembly=Sidekick.Localization"
    Width="1200"
    Height="980"
    AllowsTransparency="True"
    Background="Transparent"
    BorderThickness="1"
    ResizeMode="NoResize"
    ShowInTaskbar="False"
    Topmost="True"
    WindowStyle="None"
    mc:Ignorable="d">

    <Window.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="HighValueColor" Color="#FF336303" />
            <SolidColorBrush x:Key="MediumValueColor" Color="#FFBFA500" />
            <SolidColorBrush x:Key="LowValueColor" Color="#FFDA4500" />
            <SolidColorBrush x:Key="VeryLowValueColor" Color="#FFB00000" />
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TabControl
            x:Name="tabControlLeagueOverlay"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="{DynamicResource SolidColorBrush_Background}"
            BorderBrush="{DynamicResource SolidColorBrush_Primary}"
            Foreground="White">
            <TabItem
                x:Name="tabItemIncursion"
                Background="{DynamicResource SolidColorBrush_Background}"
                BorderBrush="{DynamicResource SolidColorBrush_Primary}"
                Header="Incursion">
                <Grid
                    Margin="0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{DynamicResource SolidColorBrush_Background}">
                    <Grid.RowDefinitions>
                        <RowDefinition Name="rowHeader" Height="25" />
                        <RowDefinition Name="rowGuardhouse" Height="30" />
                        <RowDefinition Name="rowWorkshop" Height="35" />
                        <RowDefinition Name="rowExplosivesRoom" Height="35" />
                        <RowDefinition Name="rowSplinterResearchLab" Height="50" />
                        <RowDefinition Name="rowVault" Height="30" />
                        <RowDefinition Name="rowSparringRoom" Height="35" />
                        <RowDefinition Name="rowArmourersWorkshop" Height="35" />
                        <RowDefinition Name="rowJewellersWorkshop" Height="30" />
                        <RowDefinition Name="rowSurveyorsStudy" Height="30" />
                        <RowDefinition Name="rowGemcuttersWorkshop" Height="35" />
                        <RowDefinition Name="rowTormentCells" Height="35" />
                        <RowDefinition Name="rowStrongboxChamber" Height="35" />
                        <RowDefinition Name="rowHallOfMettle" Height="50" />
                        <RowDefinition Name="rowSacrificalChamber" Height="50" />
                        <RowDefinition Name="rowStorageRoom" Height="35" />
                        <RowDefinition Name="rowCorruptionChamber" Height="30" />
                        <RowDefinition Name="rowShrineOfEmpowerment" Height="30" />
                        <RowDefinition Name="rowTempestGenerator" Height="30" />
                        <RowDefinition Name="rowPoisionGarden" Height="35" />
                        <RowDefinition Name="rowTrapWorkshop" Height="35" />
                        <RowDefinition Name="rowPoolsOfRestoration" Height="35" />
                        <RowDefinition Name="rowFlameWorkshop" Height="30" />
                        <RowDefinition Name="rowLightningWorkshop" Height="30" />
                        <RowDefinition Name="rowHatchery" Height="30" />
                        <RowDefinition Name="rowRoyalMeetingRom" Height="30" />
                        <RowDefinition Name="rowIncursionLegend" Height="25" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="columnLevel1" Width="140" />
                        <ColumnDefinition x:Name="columnLevel2" Width="175" />
                        <ColumnDefinition x:Name="columnLevel3" Width="150" />
                        <ColumnDefinition x:Name="columnContains" Width="300" />
                        <ColumnDefinition x:Name="columnModifiers" Width="260" />
                    </Grid.ColumnDefinitions>
                    <!--  Header  -->
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="0"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 1"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="1"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 2"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="2"
                        Stroke="Black" />
                    <Label
                        Grid.Row="0"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Level 3"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="3"
                        Stroke="Black" />
                    <Label
                        Name="labelIncursionHeaderContains"
                        Grid.Row="0"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Contains"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="0"
                        Grid.Column="4"
                        Stroke="Black" />
                    <Label
                        Name="labelIncursionHeaderModifiers"
                        Grid.Row="0"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Modifiers"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Legend  -->
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="0"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="Legend"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="1"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendMoreValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource HighValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="2"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource MediumValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="3"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendLessValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource LowValueColor}" />
                    <Rectangle
                        Grid.Row="26"
                        Grid.Column="4"
                        Stroke="Black" />
                    <Label
                        Grid.Row="26"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Content="{x:Static r:LeagueResources.LegendNotValuable}"
                        FontWeight="Bold"
                        Foreground="{DynamicResource VeryLowValueColor}" />
                    <!--  Guardhouse  -->
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockGuardhouse"
                        Grid.Row="1"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Guardhouse</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="1"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockBarracks"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Barracks</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="2"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHallOfWar"
                        Grid.Row="1"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Hall of War</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <Rectangle
                        Grid.Row="1"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockGuardhouseModifiers"
                        Grid.Row="1"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>+10/20/30 % Monster Pack Size</Span>
                    </TextBlock>
                    <!--  Workshop  -->
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="0"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshop"
                        Grid.Row="2"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Workshop</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="1"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockEngineeringDepartment"
                        Grid.Row="2"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Engineering Department</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="2"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockFactory"
                        Grid.Row="2"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Factory</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshopContains"
                        Grid.Row="2"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Criticals Rank 3 Recipe</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="2"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWorkshopModifiers"
                        Grid.Row="2"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>5/10/15 % increased Quantity of Items</Span>
                        <LineBreak />
                        <Span>Unique Boss has 15/25/35 % increased Life</Span>
                    </TextBlock>
                    <!--  Explosives Room  -->
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockExplosivesRoom"
                        Grid.Row="3"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Explosives Room</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="1"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockDemolitionLab"
                        Grid.Row="3"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Demolition Lab</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="2"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockShrineOfUnmaking"
                        Grid.Row="3"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Shrine of Unmaking</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockExplosivesRoomContains"
                        Grid.Row="3"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>1/2/3 Explosive Kegs</Span>
                        <LineBreak />
                        <Span>[Used to unlock doors]</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="3"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="3"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Splinter Research Lab  -->
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSplinterReasearchLab"
                        Grid.Row="4"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Splinter Research Lab</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="1"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockBreachContainmentChamber"
                        Grid.Row="4"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Breach Containment Chamber</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="2"
                        Fill="{DynamicResource HighValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHouseOfTheOthers"
                        Grid.Row="4"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>House of the Others</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSplinterResearchLabContains"
                        Grid.Row="4"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Level 1: 2 Clasped Hands</Span>
                        <LineBreak />
                        <Span>Level 2: 1 Breach</Span>
                        <LineBreak />
                        <Span>Level 3: 3 Breaches</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="4"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Vault  -->
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="0"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockVault"
                        Grid.Row="5"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Vault</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="1"
                        Fill="{DynamicResource MediumValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockTreasury"
                        Grid.Row="5"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Treasury</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="2"
                        Fill="{DynamicResource HighValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockWealthOfTheVaal"
                        Grid.Row="5"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Wealth of the Vaal</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockVaultContains"
                        Grid.Row="5"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Currency Items, including stacks and shards</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="5"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Grid.Row="5"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}" />
                    <!--  Sparring Room  -->
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="0"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSparringRoom"
                        Grid.Row="6"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Sparring Room</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="1"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArenaOfValour"
                        Grid.Row="6"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Arena of Valour</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="2"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockHallOfChampions"
                        Grid.Row="6"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Hall of Champions</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSparringRoomContains"
                        Grid.Row="6"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Weapons</Span>
                        <LineBreak />
                        <Span>Accuracy Rank 3 Recipe</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="6"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockSparringRoomModifiers"
                        Grid.Row="6"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>10/15/20 % increased Monster Damage</Span>
                    </TextBlock>
                    <!--  Armourer's Workshop  -->
                    <Rectangle
                        Grid.Row="7"
                        Grid.Column="0"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArmourersWorkshop"
                        Grid.Row="7"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Armourer's Workshop</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="7"
                        Grid.Column="1"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArmoury"
                        Grid.Row="7"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Armoury</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="7"
                        Grid.Column="2"
                        Fill="{DynamicResource VeryLowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockChamberOfIron"
                        Grid.Row="7"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Chamber of Iron</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="7"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArmourersWorkshopContains"
                        Grid.Row="7"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Armour</Span>
                        <LineBreak />
                        <Span>Animate Guardian Recipe</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="7"
                        Grid.Column="4"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockArmourersWorkshopModifiers"
                        Grid.Row="7"
                        Grid.Column="4"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>10/20/30 % more Monster Life</Span>
                    </TextBlock>
                    <!--  Jeweller's Workshop  -->
                    <Rectangle
                        Grid.Row="8"
                        Grid.Column="0"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockJewellersWorkshop"
                        Grid.Row="8"
                        Grid.Column="0"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Jeweller's Workshop</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="8"
                        Grid.Column="1"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockJewelleryForge"
                        Grid.Row="8"
                        Grid.Column="1"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Jewellery Forge</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="8"
                        Grid.Column="2"
                        Fill="{DynamicResource LowValueColor}"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockGlitteringHalls"
                        Grid.Row="8"
                        Grid.Column="2"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Glittering Halls</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="8"
                        Grid.Column="3"
                        Stroke="Black" />
                    <TextBlock
                        Name="textBlockJewellersWorkshopContains"
                        Grid.Row="8"
                        Grid.Column="3"
                        Margin="0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        FontWeight="Bold"
                        Foreground="{DynamicResource SolidColorBrush_Text}">
                        <Span>Jewellery, Talismans, Div Cards</Span>
                    </TextBlock>
                    <Rectangle
                        Grid.Row="8"
                        Grid.Column="4"
